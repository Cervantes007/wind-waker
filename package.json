{
  "name": "wind-waker",
  "version": "0.0.0-semantically-released",
  "description": "The Fastest Way To Create A Production-Ready Node.js API",
  "author": "Cervantes007",
  "scripts": {
    "api": "WW_MODE=development ts-node-dev --respawn example/src/index.ts",
    "api:client": "WW_MODE=development ts-node-dev --respawn example/src/index.ts",
    "client": "ts-node client/generate-client.ts",
    "build": "tsc",
    "commit": "git-cz",
    "docs": "cd website && yarn && yarn build",
    "docs:start": "cd website && yarn && yarn start",
    "lint": "eslint --ignore-pattern '/lib/' '*/**/*.ts' --quiet --fix",
    "semver": "semantic-release",
    "test": "jest -i",
    "test:coverage": "jest -i --coverage"
  },
  "main": "lib/index.js",
  "dependencies": {
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "cors": "2.8.5",
    "fast-safe-stringify": "2.1.0",
    "glob": "7.1.7",
    "helmet": "4.6.0",
    "pino": "6.13.0",
    "pino-http": "5.6.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/cors": "^2.8.9",
    "@types/jest": "27.0.1",
    "@types/node": "16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.1.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "ts-jest": "27.0.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "4.3.5",
    "wind-waker-client": "^1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "node",
    "nodejs",
    "framework",
    "typescript",
    "fast",
    "blazing",
    "performance",
    "decorator"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": "git@github.com:Cervantes007/wind-waker.git",
  "types": "lib/types/index.d.ts"
}

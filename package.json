{
  "name": "wind-waker",
  "version": "0.0.0-semantically-released",
  "description": "The Fastest Way To Create A Production-Ready Node.js API",
  "author": "Cervantes007",
  "scripts": {
    "api": "WW_MODE=development ts-node-dev --respawn example/index.ts",
    "api:client": "WW_MODE=development ts-node-dev --respawn example/index.ts",
    "client": "ts-node client/generate-client.ts",
    "build": "tsc",
    "commit": "git-cz",
    "docs": "yarn docs:generate && yarn docs:build",
    "docs:build": "vuepress build docs",
    "docs:cp": "cp -r vuepress/* docs/",
    "docs:dev": "vuepress dev vuepress",
    "docs:generate": "rimraf docs && typedoc && cp -r vuepress/* docs/ && cp -r vuepress/.vuepress docs",
    "docs:serve": "yarn docs && serve docs/.vuepress/dist",
    "docs:start": "yarn docs:generate && vuepress dev docs",
    "semver": "semantic-release",
    "test": "jest -i",
    "test:coverage": "jest -i --coverage"
  },
  "main": "lib/index.js",
  "dependencies": {
    "chalk": "^4.1.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "co-body": "6.1.0",
    "cors": "2.8.5",
    "glob": "^7.1.6",
    "helmet": "4.4.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/co-body": "^5.1.0",
    "@types/cors": "^2.8.9",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "axios": "^0.21.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.1",
    "typedoc": "^0.20.23",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3",
    "vuepress": "^1.8.0",
    "vuepress-plugin-one-click-copy": "^1.0.2",
    "vuepress-plugin-typedoc": "^0.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "node",
    "nodejs",
    "framework",
    "typescript",
    "fast",
    "blazing",
    "performance",
    "decorator"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": "git@github.com:Cervantes007/wind-waker.git",
  "types": "lib/types/index.d.ts"
}
